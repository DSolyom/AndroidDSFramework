		<attr name="tabSelectedIndicatorColor" format="color"/>
		<attr name="tabViewLayoutId" format="reference"/>
		<attr name="tabViewTextViewId" format="reference"/>

		<attr name="verticalScrollOnly" format="boolean" />
		<attr name="horizontalScrollOnly" format="boolean" />
